  composedCommission     Int?      @map("composed_commission")
  user                   Int?
  coordinator            Int?
  canReservate           String?   @map("can_reservate") @db.Text
  lastSession            DateTime? @map("last_session")
  createdAt              DateTime? @default(now()) @map("created_at")
