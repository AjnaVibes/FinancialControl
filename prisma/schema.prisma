  cuv               String?   @db.VarChar(255)
  street            String?   @db.VarChar(255)
  surface           Float?
  floors            Int?
  surplusMt2        Float?    @map("surplus_mt2")
  project           Int?
  price             Decimal?  @db.Decimal(10, 2)
